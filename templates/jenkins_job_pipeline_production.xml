<?xml version='1.0' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.39">
  <actions/>
  <description>A sample Jenkins pipeline</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.98">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.7.2">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>https://github.com/sivanmarom/project-flask-app.git</url>
          <credentialsId>git-credentials</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>*/main</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>production/Jenkinsfile</scriptPath>
  </definition>
   <triggers>
    <hudson.plugins.parameterizedtrigger.BuildTrigger>
      <configs>
        <hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
          <projects>Test</projects>
          <triggerWithNoParameters>false</triggerWithNoParameters>
          <triggerFromChildProjects>true</triggerFromChildProjects>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParametersConfig>
              <predefinedBuildParameters>BUILD_CAUSE=TRIGGERED_BY_PARENT_BUILD</predefinedBuildParameters>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParametersConfig>
          </configs>
        </hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
      </configs>
    </hudson.plugins.parameterizedtrigger.BuildTrigger>
  </triggers>
  <postbuilders>
    <hudson.tasks.BuildTrigger>
      <childProjects>Test</childProjects>
      <threshold>
        <name>STABLE</name>
        <ordinal>1</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </postbuilders>
</flow-definition>
